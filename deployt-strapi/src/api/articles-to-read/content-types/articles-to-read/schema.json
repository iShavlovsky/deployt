{
  "kind": "collectionType",
  "collectionName": "articles_to_reads",
  "info": {
    "singularName": "articles-to-read",
    "pluralName": "articles-to-reads",
    "displayName": "Articles to read",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "heading": {
      "type": "string"
    },
    "publicationDate": {
      "type": "date"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "articlePageCover": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "text"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "articleBody": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "standard"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "slug": {
      "type": "uid",
      "targetField": "heading",
      "required": true
    },
    "articleBody2": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "head": {
      "type": "component",
      "repeatable": false,
      "component": "test.heading"
    }
  }
}
